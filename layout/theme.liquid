  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Only initialize AOS if there are elements that need it
      if (document.querySelectorAll('[data-aos]').length > 0) {
        console.log('Found AOS elements, initializing...');
        
        // Initialize AOS
        AOS.init({
          once: false,
          mirror: true,
          offset: 100,
          duration: 800,
          easing: 'ease-in-out'
        });
        
        // Add manual refresh on scroll
        let lastScrollTop = 0;
        window.addEventListener('scroll', function() {
          const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
          
          // Reset animations when scrolling up
          if (scrollTop < lastScrollTop) {
            const animatedElements = document.querySelectorAll('[data-aos]');
            animatedElements.forEach(element => {
              const rect = element.getBoundingClientRect();
              if (rect.top > window.innerHeight) {
                element.classList.remove('aos-animate');
              }
            });
          }
          
          lastScrollTop = scrollTop <= 0 ? 0 : scrollTop;
          AOS.refresh();
        }, { passive: true });
      } else {
        console.log('No AOS elements found, skipping initialization');
      }
    });
  </script>





  


